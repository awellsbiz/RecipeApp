<%- include('../partials/header.ejs') %>
<h1> Welcome to Your profile, <%= user.name %></h1>
<p>Enjoy using our app!</p>
<p>Here are your favorite recipes:</p>


<div class="card" style="width: 18rem;">
    <img src="..." class="card-img-top" alt="...">
    <div class="card-body">
        <% userData.recipes.forEach(recipe => {%>
            <h5 class="card-title"><%= recipe.recipe.label %></h5>
            <%})%>
      <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
      <a href="#" class="btn btn-primary">Go somewhere</a>
    </div>
  </div>
   
<p>Click this link to add recipes to your profile!</p>
<a href="../recipes">Search</a>
<br />
<a href="./logout">Log Out</a>





<%- include('../partials/footer.ejs') %>